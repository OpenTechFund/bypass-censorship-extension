{
    "manifest_version": 2,

    "name": "Collateral Freedom",
    "description": "Find mirrors for blocked websites",
    "version": "1.0",

    "icons": {
        "128": "icon-128.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "storage",
        "tabs"
    ],

    "background": {
        "scripts": ["jquery.js", "eventPage.js"],
        "persistent": false
    }
}
