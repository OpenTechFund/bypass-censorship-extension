{
    "manifest_version": 2,

    "name": "Bypass Censorship",
    "description": "Find access to blocked websites",
    "version": "1.0.2",

    "icons": {
        "128": "rsc/icon-128.png"
    },

    "browser_action": {
        "browser_style": false,
        "default_icon": "rsc/icon.png",
        "default_popup": "index.html"
    },

    "options_ui": {
        "page": "options.html"
    },

    "permissions": [
        "storage",
        "tabs"
    ],

    "background": {
        "scripts": ["libs/jquery.js", "backgroundPage.js"]
    },

    "default_locale": "en",

    "browser_specific_settings": {
        "gecko": {
            "id": "{5dd64bbe-7c31-4ed8-8edf-d39252c0eaea}"
        }
    }
}
